{
    "extends": [
      "config:base"
    ],
    "platform": "github",
    "username": "feisar-bot[bot]",
    "repositories": ["auricom/containers"],
    "assignees": ["auricom"],
    "onboarding": false,
    "requireConfig": false,
    "gitAuthor": "feisar-bot <feisar-bot[bot]@users.noreply.github.com>",
    "dependencyDashboardTitle": "Renovate Dashboard ü§ñ",
    "commitMessagePrefix": "‚¨ÜÔ∏è",
    "suppressNotifications": [
      "prIgnoreNotification"
    ],
    "rebaseWhen": "conflicted",
    "packageRules": [
      {
        "description": "Auto-merge Alpine base updates",
        "matchDatasources": [
          "docker",
          "github-tags"
        ],
        "automerge": true,
        "automergeType": "branch",
        "requiredStatusChecks": null,
        "matchUpdateTypes": [
          "minor",
          "patch"
        ],
        "matchPackageNames": [
          "ghcr.io/onedr0p/alpine",
          "renovatebot/github-action"
        ]
      },
      {
        "description": "Auto-merge container digests",
        "matchDatasources": ["docker"],
        "automerge": true,
        "automergeType": "branch",
        "requiredStatusChecks": null,
        "matchUpdateTypes": ["digest"],
        "matchPackagePatterns": ["ghcr.io/onedr0p", "ghcr.io/auricom"]
      },
      {
        "description": "Auto merge GitHub Actions",
        "matchManagers": ["github-actions"],
        "matchDatasources": ["github-tags"],
        "automerge": true,
        "automergeType": "branch",
        "ignoreTests": true,
        "requiredStatusChecks": null,
        "matchUpdateTypes": ["minor", "patch"]
      },
      {
        "description": "Auto-merge python",
        "matchDatasources": ["pypi"],
        "automerge": true,
        "automergeType": "branch",
        "requiredStatusChecks": null,
        "matchUpdateTypes": ["minor", "patch"]
      }
    ]
  }